{
  "name": "runbox7",
  "homepage": "https://runbox.com",
  "bugs": {
    "url": "https://community.runbox.com"
  },
  "version": "0.1.1",
  "license": "GPL",
  "scripts": {
    "ng": "ng",
    "appdev": "ng serve --disable-host-check --base-href=/appdev/",
    "rmm6dev": "ng serve --disable-host-check --base-href=/appdev/ rmm6",
    "start": "ng serve --aot --proxy-config backend-proxy-remote.conf.js runbox7",
    "buildrmm6": "ng build --prod --base-href=/app/ rmm6",
    "build": "node updatebuildtimestamp.js && ng build --prod --base-href=/app/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib": "node ngmakelib-conf.js",
    "libwatch": "node ngmakelib-conf.js --watch",
    "publishlib": "cd .ngmakelibtmp/dist && npm publish",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37"
  },
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/pwa": "^0.7.3",
    "@angular/router": "7.1.1",
    "@angular/service-worker": "7.1.1",
    "base64-js": "^1.3.0",
    "core-js": "^2.4.1",
    "material-design-icons": "^3.0.1",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "tinymce": "4.7.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.3",
    "@angular-devkit/build-optimizer": "0.7.3",
    "@angular/cli": "7.1.2",
    "@angular/compiler-cli": "7.1.1",
    "@angular/language-service": "7.1.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngmakelib": "^0.2.6",
    "node-sass": "^4.9.3",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "3.1.6"
  }
}
